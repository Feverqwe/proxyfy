{
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19.png",
            "38": "img/icon_38.png"
        },
        "default_title": "Proxyfy",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "img/icon_128.png",
        "48": "img/icon_48.png",
        "16": "img/icon_16.png"
    },
    "options_page": "options.html",
    "permissions": ["storage", "proxy", "webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["js/bg.js"]
    },
    "name": "Proxyfy",
    "description": "Tiny proxy switcher",
    "version": "1.7.2"
}
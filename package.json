{
  "name": "proxyfy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "_clear": "del-cli ./dist",
    "_compress": "ts-node ./builder/compressDist --target ./dist/",
    "_build": "npm run _clear && webpack --config webpack.config.ts",
    "build": "npm run _build -- --mode development",
    "watch": "npm run _build -- --mode development --watch",
    "release": "npm run _build -- --mode production && npm run _compress",
    "tsc": "tsc --noEmit",
    "lint": "npx eslint ./src && npm run prettier && npm run tsc",
    "prettier": "npx prettier --check ./src",
    "prettier:fix": "npx prettier --write ./src",
    "lint:fix": "npx eslint --fix ./src && npm run prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Feverqwe/proxyfy.git"
  },
